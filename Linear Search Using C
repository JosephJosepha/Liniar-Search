#include<stdio.h>
void main()
{
	int a[20],n,i=0,key,x=0;
	printf("Enter the size of the array");
	scanf("%d",&n);
	printf("Enter the elements of the array");
	for(i=0;i<n;i++)
		scanf("%d",&a[i]);
	printf("Enter the key to be searched");
	scanf("%d",&key);
	for(i=0;i<n;i++)
	{
		if(a[i]==key)
			{
				x=1;
				break;
			}
	}
	if(x==1)
	printf("Found at position %d",i+1);
	else
	printf("Not Found");
}
